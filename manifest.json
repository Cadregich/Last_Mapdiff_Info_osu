{
  "manifest_version": 3,
  "name": "Last diff info",
  "version": "1.0",
  "description": "Add`s an information about last difficulty from mapset to maps list",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
